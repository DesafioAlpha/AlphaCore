<?xml version="1.0" encoding="UTF-8"?>
<configdata>
    <nav>
        <login>
            <label>Acesso administrativo</label>
            <module>admin</module>
            <controller>login</controller>
            <action>index</action>
            <visible>false</visible>
            <resource>guest</resource>
        </login>
        <control>
            <label>Painel de controle</label>
            <module>admin</module>
            <controller>index</controller>
            <action>index</action>
            <resource>admin</resource>
            <pages>
                <system>
                    <label>Sistema</label>
                    <module>admin</module>
                    <controller>system</controller>
                    <action>index</action>
                    <icon>system</icon>
                    <pages>
                        <calendar>
                            <label>Calendário</label>
                            <module>admin</module>
                            <controller>system</controller>
                            <action>calendar</action>
                            <icon>calendar</icon>
                        </calendar>
                        <users>
                            <label>Usuários</label>
                            <module>admin</module>
                            <controller>system</controller>
                            <action>users</action>
                            <icon>users</icon>
                        </users>
                        <logs>
                            <label>Registros</label>
                            <module>admin</module>
                            <controller>system</controller>
                            <action>logs</action>
                            <icon>logview</icon>
                        </logs>
                    </pages>
                </system>
                <reports>
                    <label>Relatórios</label>
                    <module>admin</module>
                    <controller>reports</controller>
                    <action>index</action>
                    <icon>reports</icon>
                </reports>
                <questions>
                    <label>Desafios</label>
                    <module>admin</module>
                    <controller>questions</controller>
                    <action>index</action>
                    <icon>questions</icon>
                    <pages>
                        <database>
                            <label>Banco de dados de Desafios</label>
                            <module>admin</module>
                            <controller>questions</controller>
                            <action>database</action>
                            <icon>questions_db</icon>
                            <toolbar>
                                <new_question>
                                    <icon>question</icon>
                                    <id>new_question</id>
                                    <label>Novo desafio</label>
                                </new_question>
                                <save_all>
                                    <icon>save_all</icon>
                                    <id>save_all</id>
                                    <label>Salvar tudo</label>
                                </save_all>
                            </toolbar>
                        </database>
                        <view>
                            <label>Ver desafio</label>
                            <module>admin</module>
                            <controller>questions</controller>
                            <action>view</action>
                        </view>
                        <new>
                            <label>Novo desafio</label>
                            <module>admin</module>
                            <controller>questions</controller>
                            <action>new</action>
                        </new>
                    </pages>
                </questions>
            </pages>
        </control>
    </nav>
</configdata>
